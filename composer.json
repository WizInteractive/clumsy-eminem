{
    "name": "clumsy/eminem",
    "description": "Media manager for Laravel",
    "keywords": ["laravel", "cms", "media", "images"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomas Buteler",
            "email": "tbuteler@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "5.1.*",
        "laravelcollective/html": "5.1.*",
        "intervention/image": "2.*",
        "clumsy/assets": "0.7.*"
    },
    "autoload": {
        "classmap": [
            "database/migrations"
        ],
        "psr-0": {
            "Clumsy\\Eminem\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
